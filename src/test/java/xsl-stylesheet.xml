		<fo:block font-size="{$headlineFontSize}" font-weight="bold" margin-top="3mm" margin-bottom="3mm" margin-left="30mm">  <!-- check margin -->
			INDUSTRY KNOWLEDGE
		</fo:block>
		<fo:list-block>
			<xsl:apply-templates select="cv:Skill" />
		</fo:list-block>
	</xsl:template>
