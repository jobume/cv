	<xsl:template match="cv:Employments">
		<fo:block font-size="{$headlineFontSize}" font-weight="bold" margin-top="3mm" margin-bottom="3mm" margin-left="30mm"> <!-- check margin -->
			<xsl:choose>
				<xsl:when test="//@language = 'en'">
					EMPLOYMENT
				</xsl:when>
				<xsl:otherwise>
					ANSTÄLLNINGAR
				</xsl:otherwise>
			</xsl:choose>
		</fo:block>
		<fo:list-block>
			<xsl:apply-templates select="cv:Job" />
		</fo:list-block>
	</xsl:template>

	<xsl:template match="cv:Educations">
		<fo:block font-size="{$headlineFontSize}" font-weight="bold" margin-bottom="3mm" margin-left="30mm">
			<xsl:choose>
				<xsl:when test="//@language = 'en'">
					EDUCATION
				</xsl:when>
				<xsl:otherwise>
					UTBILDNING
				</xsl:otherwise>
			</xsl:choose>
		</fo:block>
		<fo:list-block>
			<xsl:apply-templates select="cv:Acquisition" />
		</fo:list-block>
	</xsl:template>

	<xsl:template match="cv:Skill|cv:Language">
		<fo:list-item>
			<fo:list-item-label start-indent="1mm">
				<fo:block>&#8226;</fo:block>
			</fo:list-item-label>
			<fo:list-item-body start-indent="4mm">
				<fo:block>
					<xsl:value-of select="cv:Name" />&#160;<fo:inline font-size="6px" color="{$borderColor}">&#8226;</fo:inline>&#160;<xsl:value-of select="cv:Level" />
				</fo:block>
			</fo:list-item-body>
		</fo:list-item>
	</xsl:template>

	<xsl:template match="cv:Acquisition">
		<fo:list-item border-bottom="{$border}">
			<fo:list-item-label>
				<fo:block>
					<xsl:value-of select="cv:Date" />
				</fo:block>
			</fo:list-item-label>
			<fo:list-item-body start-indent="30mm">
				<fo:block font-weight="bold">
					<xsl:value-of select="cv:Name" />
				</fo:block>
			</fo:list-item-body>
		</fo:list-item>
		<fo:list-item>
